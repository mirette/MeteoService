<!-- MeteoService BPEL Process [Generated by the Eclipse BPEL Designer] -->
<!-- Date: Wed Feb 05 11:07:09 CET 2014 -->
<bpel:process name="MeteoService"
         targetNamespace="http://meteoService"
         suppressJoinFailure="yes"
         xmlns:tns="http://meteoService"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns="http://www.webserviceX.NET" xmlns:ns0="http://www.w3.org/2001/XMLSchema">

    <!-- Import the client WSDL -->
    <bpel:import namespace="http://www.webserviceX.NET" location="http://www.webservicex.net/globalweather.asmx?WSDL" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
    <bpel:import location="MeteoServiceArtifacts.wsdl" namespace="http://meteoService" 
	        importType="http://schemas.xmlsoap.org/wsdl/" />
         
    <!-- ================================================================= -->         
    <!-- PARTNERLINKS                                                      -->
    <!-- List of services participating in this BPEL process               -->
    <!-- ================================================================= -->         
    <bpel:partnerLinks>
        <!-- The 'client' role represents the requester of this service. -->
        <bpel:partnerLink name="client"
                     partnerLinkType="tns:MeteoService"
                     myRole="MeteoServiceProvider"
                     />
        
        
        <bpel:partnerLink name="GetWeather" partnerLinkType="tns:GetCity" partnerRole="GetCity"></bpel:partnerLink>
        <bpel:partnerLink name="GetWeatherPL" partnerLinkType="tns:GetWeatherPLTypeName" partnerRole="GetWeatherPLRole"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:MeteoServiceRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:MeteoServiceResponseMessage"/>
        <bpel:variable name="GetWeatherResponse" messageType="ns:GetWeatherHttpGetOut"></bpel:variable>
        <bpel:variable name="GetWeatherRequest" messageType="ns:GetWeatherHttpGetIn"></bpel:variable>
        <bpel:variable name="CityName" type="ns0:string">
        </bpel:variable>
        <bpel:variable name="CountryName" type="ns0:string"></bpel:variable>
        <bpel:variable name="Body" element="ns:string"></bpel:variable>
        <bpel:variable name="GetWeatherResponse1" messageType="ns:GetWeatherHttpGetOut"></bpel:variable>
        <bpel:variable name="GetWeatherRequest1" messageType="ns:GetWeatherHttpGetIn"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in MeteoService.wsdl 
             -->
        <bpel:receive name="receiveClientRequest" partnerLink="client"
                 portType="tns:MeteoService"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="AssignCityAndCountry">
            
            
            
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve">Lille</bpel:literal>
                </bpel:from>
                <bpel:to variable="CityName"></bpel:to>
            </bpel:copy>
            
            <bpel:copy>
                <bpel:from>
                    <bpel:literal xml:space="preserve">France : String</bpel:literal>
                </bpel:from>
                <bpel:to variable="CountryName"></bpel:to>
            </bpel:copy>
        </bpel:assign>
        
        
        
        
        
        <bpel:invoke name="GetWeather" partnerLink="GetWeather" operation="GetWeather" portType="ns:GlobalWeatherHttpGet" inputVariable="GetWeatherRequest1" outputVariable="GetWeatherResponse1">
        </bpel:invoke>
        
        <bpel:reply name="replyToClient" 
               partnerLink="client"
               portType="tns:MeteoService"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

