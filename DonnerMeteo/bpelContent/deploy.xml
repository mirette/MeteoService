<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:donnerMeteo="http://donnerMeteo" xmlns:www.webserviceX.NET="http://www.webserviceX.NET">
  <process name="donnerMeteo:DonnerMeteo">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="donnerMeteo:DonnerMeteoService" port="DonnerMeteoPort"/>
    </provide>
    <provide partnerLink="GlobalWeatherCallback">
      <service name="www.webserviceX.NET:GlobalWeather" port="GlobalWeatherSoap"/>
    </provide>
    <invoke partnerLink="GlobalWeather">
      <service name="www.webserviceX.NET:GlobalWeather" port="GlobalWeatherSoap"/>
    </invoke>
  </process>
</deploy>